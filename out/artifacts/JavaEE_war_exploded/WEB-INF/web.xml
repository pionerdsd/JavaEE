<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         id="WebApp_ID" version="3.1">

   <display-name>Index</display-name>

     <welcome-file-list>
         <welcome-file>index.jsp</welcome-file>
     </welcome-file-list>

    <servlet>
        <servlet-name>index</servlet-name>
        <jsp-file>/index.jsp</jsp-file>
    </servlet>

    <servlet-mapping>
        <servlet-name>index</servlet-name>
        <url-pattern>/index</url-pattern>
    </servlet-mapping>



    <servlet>
        <servlet-name>authorization</servlet-name>
        <jsp-file>/authorization.jsp</jsp-file>
    </servlet>

    <servlet-mapping>
        <servlet-name>authorization</servlet-name>
        <url-pattern>/authorization</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>authhead</servlet-name>
        <jsp-file>/authhead.jsp</jsp-file>
    </servlet>

    <servlet-mapping>
        <servlet-name>authhead</servlet-name>
        <url-pattern>/authhead</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>registration</servlet-name>
        <jsp-file>/registration.jsp</jsp-file>
    </servlet>

    <servlet-mapping>
        <servlet-name>registration</servlet-name>
        <url-pattern>/registration</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>registrationform</servlet-name>
        <jsp-file>/registrationform.jsp</jsp-file>
    </servlet>

    <servlet-mapping>
        <servlet-name>registrationform</servlet-name>
        <url-pattern>/registrationform</url-pattern>
    </servlet-mapping>

